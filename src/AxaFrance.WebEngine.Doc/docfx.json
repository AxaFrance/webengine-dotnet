{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\AxaFrance.WebEngine"
        },
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\AxaFrance.WebEngine.Web"
        },
        {
          "files": [
            "**.csproj"
          ],
          "src": "..\\AxaFrance.WebEngine.MobileApp"
        }

      ],
      "dest": "api_net",
      "filter": "filterConfig.yml",
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "AXA WebEngine Test Framework",
      "_appFooter": "Copyright &reg; 2016-2022 AXA France, All rights reserved.",
      "_appLogoPath": "images/logo.svg"
    },
    "content": [
      {
        "files": [
          "api_net/**.yml",
          "api_net/index.md"
        ]
      },
      {
        "files": [
          "api_java/**.yml",
          "api_java/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "tutorials/**.md",
          "tutorials/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "files/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "api_net/**.md",
          "api_java/**.md",
          "namespaces/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "../../_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false
  }
  
}