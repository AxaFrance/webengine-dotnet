<ui:BasicWindow  xmlns:ui="clr-namespace:Hummingbird.UI;assembly=Hummingbird.UI" x:Class="AXA.WebEngine.ReportViewer.WindowViewXML"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AXA.WebEngine.ReportViewer" xmlns:avalonedit="http://icsharpcode.net/sharpdevelop/avalonedit" LightIcon="icons\reportViewer.ico" DarkIcon="icons\reportViewer.ico"
        mc:Ignorable="d"
        Title="RAW XML Report" Height="450" Width="800">
    <avalonedit:TextEditor SyntaxHighlighting="XML" Background="{DynamicResource BackgroundBrush}" Foreground="{DynamicResource ForegroundBrush}"  x:Name="txtXmlReport" FontFamily="Consolas" ShowLineNumbers="True" FontSize="14" IsReadOnly="True">
        <avalonedit:TextEditor.Options>
            <avalonedit:TextEditorOptions>
                <avalonedit:TextEditorOptions.EnableHyperlinks>false</avalonedit:TextEditorOptions.EnableHyperlinks>
            </avalonedit:TextEditorOptions>
        </avalonedit:TextEditor.Options>
    </avalonedit:TextEditor>
</ui:BasicWindow>
